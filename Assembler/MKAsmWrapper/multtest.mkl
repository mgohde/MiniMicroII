const val1 10
const val2 10

>main
r1 <- #val1
r2 <- #val2
call #mult
r1 = r3
r1 = r3
call #printhex
hlt

>mult
push r4
r4 = 0
r3 = 0
for r3 to r2
	r4 = r4 + r1
endfor
r3 = r4
pop r4
ret

>printhex
r4 = 48
r5 = 15
r6 = 12

r2 = r5 << r6
r3 = r1 & r2
r3 = r3 >> r6
r3 = r3 + r4
r3 -> 8002
r3 = 1
r3 -> 8000
ret
